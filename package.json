  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
